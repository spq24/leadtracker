<div id="main">     
<div class="container">
    <table cellpadding="0" cellspacing="0" class="table table-expand table-responsive" id="leads_table">
        <thead>
        <tr>
            <th>Name</th>
            <th>Created On</th>
         </tr>

         <% if @user.company.present? %>
            <% @company_users.each do |u| %>
                <tr>
                    <td><%= u.first_name %> <%= u.last_name %></td>
                    <td><%= u.created_at.strftime("%m/%d/%Y") %></td>
                    <td><%= link_to "edit", edit_user_path(u) %> | <em><%= link_to "delete", u, method: :delete, style: "color:red;" %></em></td>
                </tr>
            <% end %>
        <% else %>
            <% @agency_users.each do |u| %>
                <tr>
                    <td><%= u.first_name %> <%= u.last_name %></td>
                    <td><%= u.created_at.strftime("%m/%d/%Y") %></td>
                    <td><%= link_to "edit", edit_user_path(u) %> | <em><%= link_to "delete", u, method: :delete, style: "color:red;" %></em></td>
                </tr>
            <% end %>
        <% end %>
        </thead> 
    </table>

    <%= link_to "Add a User", new_user_path, class: "btn btn-green btn-wide btn-large pull-right" %>